    },
    "homepage": "https://github.com/dev-nguyenhoanganh/database-versioning#readme",
    "dependencies": {
        "dotenv": "^16.0.3",
        "express": "^4.18.2",
        "mysql": "^2.18.1",
        "mysql2": "^3.3.1",
        "pg": "^8.10.0",
        "pg-hstore": "^2.3.4",
        "sequelize": "^6.31.1",
        "sequelize-cli": "^6.6.0",
        "sequelize-typescript": "^2.1.5",
        "sequelize-typescript-migration-lts": "^3.2.5",
        "ts-node": "^10.9.1"
    },
